<p id="notice"><%= notice %></p>

<h1>User Interests</h1>

<table>
  <thead>
    <tr>
      <th>Interest</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_interests.each do |user_interest| %>
      <tr>
        <td><%= user_interest.interest_id %></td>
        <td><%= user_interest.user_id %></td>
        <td><%= link_to 'Show', user_interest %></td>
        <td><%= link_to 'Edit', edit_user_interest_path(user_interest) %></td>
        <td><%= link_to 'Destroy', user_interest, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Interest', new_user_interest_path %>
